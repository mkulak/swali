{
  "swagger": "2.0",
  "info": {
    "version": "1.0.0",
    "title": "Swagger Petstore",
    "description": "A sample API that uses a petstore as an example to demonstrate features in the swagger-2.0 specification",
    "termsOfService": "http://swagger.io/terms/",
    "contact": {
      "name": "Swagger API Team"
    },
    "license": {
      "name": "MIT"
    }
  },
  "host": "petstore.swagger.io",
  "basePath": "/api",
  "schemes": [
    "http"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/pets": {
      "post": {
        "description": "Creates a new pet in the store.  Duplicates are allowed",
        "operationId": "addPet",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "pet",
            "in": "body",
            "description": "Pet to add to the store",
            "required": true,
            "schema": {
              "$ref": "#/definitions/NewPet"
            }
          },
          {
            "name": "Authorization",
            "in": "Header",
            "type": "string",
            "description": "Bearer authorization token",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "ref to an object",
            "schema": {
              "$ref": "#/definitions/Pet"
            }
          },
          "201": {
            "description": "inline object",
            "schema": {
              "type": "object",
              "properties": {
                "someProperty": {
                  "type": "string"
                }
              }
            }
          },
          "202": {
            "description": "ref to a composed object allOf",
            "schema": {
              "$ref": "#/definitions/PetAllOf"
            }
          },
          "203": {
            "description": "ref to an object oneOf",
            "schema": {
              "$ref": "#/definitions/PetOneOf"
            }
          },
          "204": {
            "description": "ref to an object anyOf",
            "schema": {
              "$ref": "#/definitions/PetAnyOf"
            }
          }
        }
      }
    }
  },
  "definitions": {
    "Pet": {
      "type": "object",
      "required": [
        "id",
        "name"
      ],
      "properties": {
        "id": {
          "type": "integer",
          "format": "int64"
        },
        "name": {
          "type": "string"
        },
        "tag": {
          "type": "string"
        }
      }
    },
    "PetAllOf": {
      "allOf": [
        {
          "$ref": "#/definitions/Pet"
        },
        {
          "properties": {
            "additional": {
              "type": "string",
              "description": "string"
            }
          }
        }
      ]
    },
    "PetOneOf": {
      "type": "object",
      "oneOf": [
        {
          "$ref": "#/definitions/Pet"
        },
        {
          "properties": {
            "additional": {
              "type": "string",
              "description": "string"
            }
          }
        }
      ]
    },
    "PetAnyOf": {
      "type": "object",
      "anyOf": [
        {
          "$ref": "#/definitions/Pet"
        },
        {
          "properties": {
            "additional": {
              "type": "string",
              "description": "string"
            }
          }
        }
      ]
    }
  }
}